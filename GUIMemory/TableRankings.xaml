<Window x:Class="GUIMemory.TableRankings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUIMemory"
        mc:Ignorable="d"
        Title="TableRankings" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="label" HorizontalAlignment="Left" Margin="288,10,0,0" TextWrapping="Wrap" Text="Enter your name to upload your scrore" VerticalAlignment="Top"/>
        <TextBox x:Name="NameBox" HorizontalAlignment="Left" Margin="238,31,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="217" Height="19"/>
        <Button x:Name="button" Content="Upload score" HorizontalAlignment="Left" Margin="460,30,0,0" VerticalAlignment="Top" Width="84" Click="Button_Click"/>

        <Grid x:Name="TableGrid" Margin="0,20,0,0" VerticalAlignment="Center" HorizontalAlignment="Center">

        </Grid>
        <Button x:Name="ViewImagesBtn" Content="View Images" Margin="589,372,35,10" Click="ViewImagesBtn_Click" Visibility="Hidden"/>

        <Button x:Name="NewGame" Content="New Game" Margin="312,372,312,10" Click="NewGame_Click" Visibility="Hidden"/>

    </Grid>
</Window>
